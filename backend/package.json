{
  "name": "nfl-pool-app-backend",
  "version": "1.0.0",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js --runInBand --detectOpenHandles --forceExit",
    "test:models": "cross-env NODE_ENV=test jest --config jest.config.js --runInBand --detectOpenHandles --forceExit __tests__/models",
    "test:models:watch": "cross-env NODE_ENV=test jest --config jest.config.js --watch --detectOpenHandles __tests__/models",
    "test:services": "cross-env NODE_ENV=test jest --config jest.config.js --runInBand --detectOpenHandles --forceExit __tests__/services",
    "test:services:watch": "cross-env NODE_ENV=test jest --config jest.config.js --watch --detectOpenHandles __tests__/services",
    "test:controllers": "cross-env NODE_ENV=test jest --config jest.config.js --runInBand --detectOpenHandles --forceExit __tests__/controllers",
    "test:controllers:watch": "cross-env NODE_ENV=test jest --config jest.config.js --watch --detectOpenHandles __tests__/controllers",
    "test:coverage": "cross-env NODE_ENV=test jest --config jest.config.js --coverage --detectOpenHandles --forceExit",
    "docs": "jsdoc -c jsdoc.json",
    "init-nfl-data": "node scripts/initializeNFLData.js",
    "generate-swagger": "node -e \"const fs = require('fs'); const swaggerSpec = require('./config/swaggerOptions'); fs.writeFileSync('swagger.json', JSON.stringify(swaggerSpec, null, 2));\""
  },
  "dependencies": {
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.4.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.5.1",
    "express-rate-limit": "^5.2.6",
    "express-validator": "^7.1.0",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mongoose": "^5.12.3",
    "nock": "^13.5.4",
    "nodemailer": "^6.9.14",
    "readline": "^1.3.0",
    "sanitize": "^2.1.2",
    "sanitize-html": "^2.13.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "jsdoc": "^3.6.7",
    "mongodb-memory-server": "^9.4.1",
    "supertest": "^7.0.0"
  },
  "main": "server.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
